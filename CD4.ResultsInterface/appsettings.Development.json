{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IncomingDirectory": "C:\\Export",
  "ControlFileExtension": "*.ok",
  "DataFileExtension": ".json",
  "WaitIntervalMilliseconds": 1000,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\CD4.ResultInterface_.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15
        }
      }
    ]
  }
}
